        "dist/movies/*",
        "dist/movies/*"
      ],
      "discover": [
        "dist/discover"
      ],
      "discover/*": [
        "dist/discover/*"
      ],
      "reviews": [
        "dist/reviews",
        "dist/reviews"
      ],
      "reviews/*": [
        "dist/reviews/*",
        "dist/reviews/*"
      ],
      "shared": [
        "dist/shared"
      ],
      "shared/*": [
        "dist/shared/*"
      ],
      "auth": [
        "dist/auth"
      ],
      "auth/*": [
        "dist/auth/*"
      ],
      "search": [
        "dist/search"
      ],
      "search/*": [
        "dist/search/*"
      ],
      "trending": [
        "dist/trending"
      ],
      "trending/*": [
        "dist/trending/*"
      ],
      "home": [
        "dist/home",
        "dist/home",
        "dist/home",
        "dist/home"
      ],
      "home/*": [
        "dist/home/*",
        "dist/home/*",
        "dist/home/*",
        "dist/home/*"
      ]
    }
  }
}